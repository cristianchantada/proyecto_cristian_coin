CREATE TABLE "operations_table" (
	"id"	INTEGER NOT NULL UNIQUE,
	"date"	TEXT NOT NULL,
	"time"	TEXT NOT NULL,
	"moneda_from"	TEXT NOT NULL,
	"cantidad_from"	REAL NOT NULL,
	"moneda_to"	TEXT NOT NULL,
	"cantidad_to"	REAL NOT NULL,
	"unitary_prize"	INTEGER NOT NULL,
	PRIMARY KEY("id" AUTOINCREMENT)
);

CREATE TABLE "stand_by_operation_table" (
	"moneda_from"	TEXT,
	"quantity_from"	INTEGER,
	"moneda_to"	TEXT,
	"quantity_to"	INTEGER,
	"date"	TEXT,
	"time"	INTEGER
, "unitary_prize"	INTEGER);